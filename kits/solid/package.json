{
  "type": "module",
  "name": "@anchorkit/solid",
  "version": "1.0.0-beta.19",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./actions": {
      "types": "./dist/actions/index.d.ts",
      "import": "./dist/actions/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "import": "./dist/icons/index.js"
    },
    "./layouts": {
      "types": "./dist/layouts/index.d.ts",
      "import": "./dist/layouts/index.js"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js"
    }
  },
  "files": ["dist"],
  "directories": {
    "dist": "./dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "esbuild-raw-plugin": "catalog:",
    "publint": "catalog:",
    "rimraf": "catalog:",
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "solid-js": "catalog:lib",
    "tailwindcss": "catalog:lib",
    "typescript": "catalog:lib"
  },
  "scripts": {
    "dev": "rimraf dist && tsdown --watch",
    "clean": "rimraf dist",
    "build": "rimraf dist && tsdown && publint",
    "prepublish": "rimraf dist && tsdown && publint",
    "format": "biome format --write"
  },
  "license": "MIT",
  "dependencies": {
    "@anchorkit/headless": "workspace:*",
    "@anchorlib/solid": "workspace:*"
  }
