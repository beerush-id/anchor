{
  "type": "module",
  "name": "@anchorkit/headless",
  "version": "1.0.0-beta.12",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./actions": {
      "types": "./dist/actions/index.d.ts",
      "import": "./dist/actions/index.js"
    },
    "./actions/*": "./dist/actions/*",
    "./states": {
      "types": "./dist/states/index.d.ts",
      "import": "./dist/states/index.js"
    },
    "./states/*": "./dist/states/*",
    "./styles/*": "./dist/styles/*",
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./utils/*": "./dist/utils/*"
  },
  "files": [
    "dist"
  ],
  "directories": {
    "dist": "./dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eslint/css": "catalog:",
    "@eslint/js": "catalog:",
    "@eslint/markdown": "catalog:",
    "esbuild-raw-plugin": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "jsdom": "catalog:",
    "prettier": "catalog:",
    "publint": "catalog:",
    "rimraf": "catalog:",
    "tailwindcss": "catalog:lib",
    "tsup": "catalog:",
    "typescript-eslint": "catalog:",
    "@biomejs/biome": "catalog:"
  },
  "peerDependencies": {
    "tailwindcss": "catalog:lib",
    "typescript": "catalog:lib"
  },
  "scripts": {
    "dev": "rimraf dist && tsup --watch",
    "clean": "rimraf dist",
    "build": "rimraf dist && tsup && publint",
    "prepublish": "rimraf dist && tsup && publint",
    "format": "prettier --write .",
    "test": "rimraf coverage && vitest --run",
    "test:preview": "rimraf coverage && vitest --run && vite preview --outDir coverage"
  },
  "license": "MIT",
  "dependencies": {
    "@anchorlib/core": "workspace:*"
  }
}
