{
  "type": "module",
  "name": "@anchorkit/react",
  "version": "1.0.0-beta.11",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./actions": {
      "types": "./dist/actions/index.d.ts",
      "import": "./dist/actions/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./components/*": "./dist/components/*",
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "import": "./dist/icons/index.js"
    },
    "./icons/*": "./dist/icons/*",
    "./layouts": {
      "types": "./dist/layouts/index.d.ts",
      "import": "./dist/layouts/index.js"
    },
    "./layouts/*": "./dist/layouts/*",
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js"
    },
    "./lib/*": "./dist/lib/*"
  },
  "files": [
    "dist"
  ],
  "directories": {
    "dist": "./dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eslint/css": "^0.5.0",
    "@eslint/js": "^9.23.0",
    "@eslint/markdown": "^6.3.0",
    "@types/react": "^19.2.2",
    "esbuild-raw-plugin": "^0.2.0",
    "eslint": "^9.23.0",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.3",
    "publint": "^0.3.9",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.14",
    "tsup": "^8.4.0",
    "typescript-eslint": "^8.27.0"
  },
  "peerDependencies": {
    "tailwindcss": "^4.1.14",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "dev": "rimraf dist && tsup --watch",
    "clean": "rimraf dist",
    "build": "rimraf dist && tsup && publint",
    "prepublish": "rimraf dist && tsup && publint",
    "format": "prettier --write .",
    "test": "rimraf coverage && vitest --run",
    "test:preview": "rimraf coverage && vitest --run && vite preview --outDir coverage"
  },
  "license": "MIT",
  "optionalDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "dependencies": {
    "@anchorkit/headless": "workspace:*",
    "@anchorlib/core": "workspace:*",
    "@anchorlib/react": "workspace:*"
  }
}
