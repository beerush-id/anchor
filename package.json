{
  "name": "anchorlib",
  "type": "module",
  "version": "1.0.0-beta.11",
  "description": "Reactive State Management for Javascript",
  "scripts": {
    "dev": "bpkg run dev",
    "dev:core": "bpkg run dev -f packages/core",
    "dev:next": "bpkg run dev -f apps/next",
    "dev:react": "bpkg run dev -f apps/react",
    "dev:svelte": "bpkg run dev -f apps/svelte",
    "dev:vue": "bpkg run dev -f apps/vue",
    "build:packages": "bpkg run build -f packages/*",
    "build:next": "bpkg run build -f apps/next --sequential",
    "build:react": "bpkg run build -f apps/react --sequential",
    "build": "bpkg run build -f packages/* --sequential",
    "prepublish": "bpkg run prepublish -f packages/* --sequential",
    "format": "bpkg run format -f apps/* packages/* && git add --renormalize .",
    "lint": "bpkg run lint",
    "test": "rimraf apps/next/public/coverage && vitest --run",
    "test:all": "mpkg run test -f packages/* --sequential",
    "test:preview": "rimraf apps/next/public/coverage && vitest --run && vite preview --outDir apps/next/public/coverage",
    "prepare": "husky",
    "docs:dev": "vitepress dev docs --port 6611",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 5611"
  },
  "devDependencies": {
    "@anchorlib/react": "workspace:*",
    "@anchorlib/storage": "workspace:*",
    "@types/markdown-it-container": "^2.0.10",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "markdown-it-container": "^4.0.0",
    "monopkg": "^0.3.10",
    "prettier": "^3.2.5",
    "vite-plugin-solid": "^2.11.10",
    "vitepress": "^1.6.4",
    "vitepress-plugin-llms": "^1.7.5",
    "vitepress-plugin-sandpack": "^1.1.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.40.2"
  },
  "engines": {
    "bun": ">=1.3.0"
  },
  "packageManager": "bun@1.3.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "kits/*"
  ],
  "author": "Nanang Mahdaen El Agung <mahdaen@gmail.com>",
  "homepage": "https://github.com/beerush-id/anchor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beerush-id/anchor.git"
  },
  "bugs": {
    "url": "https://github.com/beerush-id/anchor/issues"
  },
  "lint-staged": {},
  "catalogs": {
    "app": {
      "@tailwindcss/vite": "4.1.16",
      "@types/react": "19.2.2",
      "postcss": "8.5.6",
      "react": "19.2.0",
      "react-dom": "19.2.0",
      "solid-js": "1.9.9",
      "svelte": "5.41.3",
      "tailwindcss": "4.1.16",
      "typescript": "5.9.3",
      "vue": "3.5.22",
      "zod": "4.1.12"
    },
    "lib": {
      "@types/react": "^19.1.0",
      "react": "^19.1.0",
      "react-dom": "^19.1.0",
      "solid-js": "^1.9.9",
      "svelte": "^5.0.0",
      "tailwindcss": "^4.0.0",
      "typescript": "^5.8.2",
      "zod": "^4.1.5"
    }
  },
  "catalog": {
    "@solidjs/testing-library": "0.8.10",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/svelte": "5.2.8",
    "@testing-library/user-event": "14.6.1",
    "@testing-library/vue": "8.1.0",
    "@types/bun": "1.3.1",
    "@vitest/coverage-v8": "4.0.2",
    "@vitest/ui": "4.0.2",
    "@zod/core": "0.11.6",
    "jsdom": "27.0.1",
    "prettier": "3.6.2",
    "publint": "0.3.15",
    "tsdown": "0.15.9",
    "vite": "7.1.12",
    "vitest": "4.0.2",
    "zod": "4.1.12"
  }
}
