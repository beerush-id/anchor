{
  "name": "anchorlib",
  "type": "module",
  "version": "1.0.0-beta.11",
  "description": "Reactive State Management for Javascript",
  "scripts": {
    "dev": "bpkg run dev",
    "dev:core": "bpkg run dev -f packages/core",
    "dev:next": "bpkg run dev -f apps/next",
    "dev:react": "bpkg run dev -f apps/react",
    "dev:svelte": "bpkg run dev -f apps/svelte",
    "dev:vue": "bpkg run dev -f apps/vue",
    "build:packages": "bpkg run build -f packages/*",
    "build:next": "bpkg run build -f apps/next --sequential",
    "build:react": "bpkg run build -f apps/react --sequential",
    "build": "bpkg run build -f packages/* --sequential",
    "prepublish": "bpkg run prepublish -f packages/* --sequential",
    "format": "bpkg run format -f apps/* packages/* && git add --renormalize .",
    "lint": "bpkg run lint",
    "test": "rimraf apps/next/public/coverage && vitest --run",
    "test:all": "mpkg run test -f packages/* --sequential",
    "test:preview": "rimraf apps/next/public/coverage && vitest --run && vite preview --outDir apps/next/public/coverage",
    "prepare": "husky",
    "docs:dev": "vitepress dev docs --port 6611",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 5611"
  },
  "devDependencies": {
    "@types/markdown-it-container": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "husky": "catalog:",
    "lint-staged": "catalog:",
    "markdown-it-container": "catalog:",
    "monopkg": "catalog:",
    "prettier": "catalog:",
    "vite-plugin-solid": "catalog:",
    "vitepress": "catalog:",
    "vitepress-plugin-llms": "catalog:",
    "vitepress-plugin-sandpack": "catalog:",
    "vitest": "catalog:",
    "wrangler": "catalog:"
  },
  "engines": {
    "bun": ">=1.3.0"
  },
  "packageManager": "bun@1.3.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "kits/*"
  ],
  "author": "Nanang Mahdaen El Agung <mahdaen@gmail.com>",
  "homepage": "https://github.com/beerush-id/anchor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beerush-id/anchor.git"
  },
  "bugs": {
    "url": "https://github.com/beerush-id/anchor/issues"
  },
