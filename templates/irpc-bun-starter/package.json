{
  "type": "module",
  "name": "hello-irpc",
  "version": "1.0.0",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./dist/rpc/*",
    "./lib/*": "./dist/lib/*"
  },
  "files": ["dist"],
  "directories": {
    "dist": "./dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@irpclib/irpc": "^1.0.0-beta.18",
    "@irpclib/http": "^1.0.0-beta.18"
  },
  "devDependencies": {
    "@types/bun": "^1.3.1",
    "publint": "^0.3.15",
    "rimraf": "^6.0.1",
    "tsdown": "^0.15.9"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "scripts": {
    "dev": "rimraf dist && tsdown --watch ./src",
    "build": "rimraf dist && tsdown",
    "serve": "bun ./src",
    "prepublish": "tsdown && publint"
  },
  "license": "MIT"
}
