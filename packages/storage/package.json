{
  "type": "module",
  "name": "@anchorlib/storage",
  "version": "1.0.0-beta.12",
  "description": "Persistent storage solutions for Anchor state management",
  "keywords": [
    "state",
    "management",
    "reactive",
    "observable",
    "storage"
  ],
  "author": "Nanang Mahdaen El Agung <mahdaen@gmail.com>",
  "homepage": "https://github.com/beerush-id/anchor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beerush-id/anchor.git"
  },
  "bugs": {
    "url": "https://github.com/beerush-id/anchor/issues"
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./db": {
      "types": "./dist/db/index.d.ts",
      "import": "./dist/db/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "directories": {
    "dist": "./dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@anchorlib/core": "workspace:*"
  },
  "devDependencies": {
    "@beerush/utils": "^0.5.0",
    "@eslint/css": "catalog:",
    "@eslint/js": "catalog:",
    "@eslint/markdown": "catalog:",
    "@playwright/test": "^1.54.2",
    "@types/bun": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "fake-indexeddb": "^6.1.0",
    "jsdom": "catalog:",
    "prettier": "catalog:",
    "publint": "catalog:",
    "rimraf": "catalog:",
    "tsup": "catalog:",
    "typescript-eslint": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "@biomejs/biome": "catalog:"
  },
  "peerDependencies": {
    "typescript": "catalog:lib"
  },
  "scripts": {
    "dev": "rimraf dist && tsup --watch",
    "clean": "rimraf dist",
    "build": "rimraf dist && tsup && publint",
    "format": "prettier --write .",
    "prepublish": "rimraf dist && tsup && publint",
    "test": "rimraf coverage && vitest --run",
    "test:preview": "rimraf coverage && vitest --run && vite preview --outDir coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui"
  },
  "optionalDependencies": {
    "zod": "catalog:lib"
  }
}
