{
  "type": "module",
  "name": "@anchorlib/react",
  "version": "1.0.0-beta.1",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": "./dist/storage/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "directories": {
    "dist": "./dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eslint/css": "^0.5.0",
    "@eslint/js": "^9.23.0",
    "@eslint/markdown": "^6.3.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.8",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.23.0",
    "eslint-plugin-prettier": "^5.2.3",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "publint": "^0.3.9",
    "react": "^19.1.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.4.0",
    "typescript-eslint": "^8.27.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "dev": "rimraf dist && tsup --watch",
    "clean": "rimraf dist",
    "build": "rimraf dist && tsup && publint",
    "prepublish": "rimraf dist && tsup && publint",
    "format": "prettier --write .",
    "test": "rimraf coverage && vitest --run",
    "test:preview": "rimraf coverage && vitest --run && vite preview --outDir coverage"
  },
  "license": "MIT",
  "dependencies": {
    "@anchorlib/core": "workspace:*",
    "@anchorlib/storage": "workspace:*"
  },
  "optionalDependencies": {
    "zod": "^4.1.5"
  }
}
