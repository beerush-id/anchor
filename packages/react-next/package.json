{
  "type": "module",
  "name": "@anchorlib/react-next",
  "version": "1.0.0-beta.12",
  "description": "React bindings for Anchor reactive state management",
  "keywords": ["state", "management", "reactive", "observable", "react"],
  "author": "Nanang Mahdaen El Agung <mahdaen@gmail.com>",
  "homepage": "https://github.com/beerush-id/anchor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beerush-id/anchor.git"
  },
  "bugs": {
    "url": "https://github.com/beerush-id/anchor/issues"
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    }
  },
  "files": ["dist"],
  "directories": {
    "dist": "./dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "publint": "catalog:",
    "react": "catalog:lib",
    "rimraf": "catalog:",
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "typescript": "catalog:lib",
    "react": "catalog:lib"
  },
  "scripts": {
    "dev": "tsdown --watch",
    "clean": "rimraf dist",
    "build": "tsdown && publint",
    "prepublish": "tsdown && publint",
    "format": "biome format --write",
    "test": "rimraf coverage && vitest --run",
    "test:preview": "rimraf coverage && vitest --run && vite preview --outDir coverage"
  },
  "license": "MIT",
  "dependencies": {
    "@anchorlib/core": "workspace:*",
    "@anchorlib/storage": "workspace:*"
  }
}
