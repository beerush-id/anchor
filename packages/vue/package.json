{
  "name": "@anchorlib/vue",
  "version": "1.0.0-beta.7",
  "description": "Vue bindings for Anchor reactive state management",
  "keywords": [
    "state",
    "management",
    "reactive",
    "observable",
    "vue"
  ],
  "author": "Nanang Mahdaen El Agung <mahdaen@gmail.com>",
  "homepage": "https://github.com/beerush-id/anchor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beerush-id/anchor.git"
  },
  "bugs": {
    "url": "https://github.com/beerush-id/anchor/issues"
  },
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": "./dist/storage/index.js"
    }
  },
  "directories": {
    "dist": "./dist"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@anchorlib/core": "workspace:*",
    "@anchorlib/storage": "workspace:*"
  },
  "devDependencies": {
    "@eslint/css": "^0.5.0",
    "@eslint/js": "^9.23.0",
    "@eslint/markdown": "^6.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.23.0",
    "eslint-plugin-prettier": "^5.2.3",
    "jsdom": "^27.0.0",
    "prettier": "^3.5.3",
    "publint": "^0.3.9",
    "rimraf": "^6.0.1",
    "tsup": "^8.4.0",
    "typescript-eslint": "^8.27.0",
    "vitest": "^3.2.4",
    "vue": "^3.5.19"
  },
  "peerDependencies": {
    "typescript": "^5.8.2",
    "vue": "^3.5.19"
  },
  "scripts": {
    "dev": "rimraf dist && tsup --watch",
    "clean": "rimraf dist",
    "build": "rimraf dist && tsup && publint",
    "format": "prettier --write .",
    "prepublish": "rimraf dist && tsup && publint",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "optionalDependencies": {
    "zod": "^4.1.5"
  }
}
