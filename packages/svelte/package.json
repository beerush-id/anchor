{
  "name": "@anchorlib/svelte",
  "version": "1.0.0-beta.20",
  "description": "Svelte bindings for Anchor reactive state management",
  "keywords": [
    "state",
    "management",
    "reactive",
    "observable",
    "svelte"
  ],
  "author": "Nanang Mahdaen El Agung <mahdaen@gmail.com>",
  "homepage": "https://github.com/beerush-id/anchor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beerush-id/anchor.git"
  },
  "bugs": {
    "url": "https://github.com/beerush-id/anchor/issues"
  },
  "type": "module",
  "module": "./dist/index.js",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./reactive": {
      "types": "./dist/reactive.d.ts",
      "svelte": "./dist/reactive.js",
      "import": "./dist/reactive.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "svelte": "./dist/core/index.js",
      "import": "./dist/core/index.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "svelte": "./dist/storage/index.js",
      "import": "./dist/storage/index.js"
    }
  },
  "directories": {
    "dist": "./dist"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@anchorlib/core": "workspace:*",
    "@anchorlib/storage": "workspace:*"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@sveltejs/vite-plugin-svelte": "catalog:",
    "@testing-library/svelte": "catalog:",
    "@testing-library/user-event": "catalog:",
    "@types/react": "catalog:lib",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "jsdom": "catalog:",
    "publint": "catalog:",
    "rimraf": "catalog:",
    "tsdown": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "svelte": "catalog:lib",
    "typescript": "catalog:lib"
  },
  "scripts": {
    "dev": "rimraf dist && tsdown --watch ./src",
    "clean": "rimraf dist",
    "build": "rimraf dist && tsdown && publint",
    "prepublish": "rimraf dist && tsdown && publint",
    "format": "biome format --write",
    "test": "rimraf coverage && vitest --run",
    "test:preview": "rimraf coverage && vitest --run && vite preview --outDir coverage"
  },
  "optionalDependencies": {
    "zod": "catalog:lib"
  }
}