{
  "type": "module",
  "name": "@anchorlib/react-kit",
  "version": "1.0.0-beta.12",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./actions": {
      "types": "./dist/actions/index.d.ts",
      "import": "./dist/actions/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "import": "./dist/icons/index.js"
    },
    "./styles/*": "./dist/styles/*",
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./view": {
      "types": "./dist/view/index.d.ts",
      "import": "./dist/view/index.js"
    }
  },
  "files": ["dist"],
  "directories": {
    "dist": "./dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@shikijs/transformers": "catalog:lib",
    "@types/bun": "catalog:",
    "@types/react": "catalog:lib",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "esbuild": "catalog:",
    "esbuild-raw-plugin": "catalog:",
    "jsdom": "catalog:",
    "lucide-react": "catalog:lib",
    "publint": "catalog:",
    "rimraf": "catalog:",
    "tailwindcss": "catalog:lib",
    "tsup": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "tailwindcss": "catalog:lib",
    "typescript": "catalog:lib"
  },
  "scripts": {
    "dev": "rimraf dist && tsup --watch",
    "clean": "rimraf dist",
    "build": "rimraf dist && tsup && publint",
    "prepublish": "rimraf dist && tsup && publint",
    "format": "biome format --write"
  },
  "license": "MIT",
  "optionalDependencies": {
    "react": "catalog:lib",
    "react-dom": "catalog:lib",
    "shiki": "catalog:lib"
  },
  "dependencies": {
    "@anchorlib/core": "workspace:*",
    "@anchorlib/react": "workspace:*",
    "@anchorlib/storage": "workspace:*"
  }
}
