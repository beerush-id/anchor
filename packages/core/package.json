{
  "type": "module",
  "name": "@anchorlib/core",
  "version": "1.0.0-beta.20",
  "description": "Core reactive state management library for Anchor",
  "keywords": [
    "state",
    "management",
    "reactive",
    "observable"
  ],
  "author": "Nanang Mahdaen El Agung <mahdaen@gmail.com>",
  "homepage": "https://github.com/beerush-id/anchor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beerush-id/anchor.git"
  },
  "bugs": {
    "url": "https://github.com/beerush-id/anchor/issues"
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "directories": {
    "dist": "./dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "@zod/core": "catalog:",
    "benny": "catalog:",
    "jsdom": "catalog:",
    "publint": "catalog:",
    "rimraf": "catalog:",
    "tsdown": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "typescript": "catalog:lib"
  },
  "scripts": {
    "dev": "rimraf dist && tsdown --watch ./src",
    "clean": "rimraf dist",
    "build": "rimraf dist && tsdown && publint",
    "prepublish": "rimraf dist && tsdown && publint",
    "test": "rimraf coverage && vitest --run",
    "test:preview": "rimraf coverage && vitest --run && vite preview --outDir coverage",
    "format": "biome format --write"
  },
  "optionalDependencies": {
    "zod": "catalog:lib"
  }
}