{
  "name": "@anchor-demo/svelte",
  "version": "1.0.0-beta.19",
  "dependencies": {
    "@anchorlib/storage": "workspace:*",
    "@anchorlib/svelte": "workspace:*"
  },
  "devDependencies": {
    "@anchorlib/core": "workspace:*",
    "@biomejs/biome": "catalog:",
    "@sveltejs/adapter-auto": "catalog:",
    "@sveltejs/kit": "catalog:",
    "@sveltejs/vite-plugin-svelte": "catalog:",
    "@tailwindcss/vite": "catalog:app",
    "@vitest/ui": "catalog:",
    "globals": "catalog:",
    "jsdom": "catalog:",
    "prettier-plugin-svelte": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:",
    "svelte": "catalog:app",
    "svelte-check": "catalog:",
    "tailwindcss": "catalog:app",
    "typescript": "catalog:app",
    "vite": "catalog:",
    "vite-plugin-devtools-json": "catalog:",
    "vitest": "catalog:",
    "zod": "catalog:"
  },
  "private": true,
  "scripts": {
    "dev": "vite dev --port 5273",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "biome format --write",
    "lint": "prettier --check . && eslint ."
  },
  "type": "module"
}